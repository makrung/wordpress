{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/ep-login-block",
  "version": "0.1.0",
  "title": "EventPrime Login",
  "category": "event-prime",
  "description": "EventPrime login description",
  "attributes": {
    "block_login_custom_class": {
      "type": "string",
      "default": "block_login_custom_class"
    },
    "block_login_title": {
      "type": "string"
    },
    "block_login_user_detail_label": {
      "type": "string"
    },
    "block_login_password_label": {
      "type": "string"
    },
    "block_login_remember_me_label": {
      "type": "string"
    },
    "block_login_forget_password_label": {
      "type": "string"
    },
    "block_login_click_here_label": {
      "type": "string"
    },
    "block_login_dont_have_account_label": {
      "type": "string"
    },
    "block_login_register_link_label": {
      "type": "string"
    },
    "align": {
      "type": "string",
      "default": "center"
    },
    "backgroundColor": {
      "type": "string",
      "default": "#fff"
    },
    "textColor": {
      "type": "string",
      "default": "#000"
    },
    "textAlign": {
      "type": "string",
      "default": "center"
    }
  },
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "type": "string",
        "default": "<img src=\"data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAmklEQVR42mP8//78/AyUBEwPD/3//Gx8VM/ADxlGAV+4CCQGwWNqWIMwFTCzJwMDAxmYGBgXAEAJhSdjAM4hx8VpP4nJxc+4JAZmmFgqZPp3iNIQE9XAAIOMhoF1OJWVAAAAAElFTkSuQmCC\" style=\"display: block; margin: 0 auto;\">"
      }
    }
  },
  "supports": {
    "mode": true,
    "html": false,
    "align": true
  },
  "textdomain": "ep-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:../../build/ep-login-block/index.css",
  "style": "file:./style-index.css"
}